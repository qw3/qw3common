<% unless @endereco.blank? %>
  $("#" + $("#tipo").val() + "_endereco_attributes_rua").val( "<%= @endereco[0] + " " + @endereco[1] %>" )
  $("#" + $("#tipo").val() + "_endereco_attributes_bairro").val( "<%= @endereco[2] %>" )
  <% estado = Estado.find_by_acronimo( @endereco[3].downcase ).id %>
  <% cidade = Cidade.where( :nome => @endereco[4], :estado_id => estado ).first.id %>
  $("#estado_id").val( "<%= estado %>" )
  $.ajax({url: '/enderecos/gerar_combo_cidade',
    type: 'POST',
    data: {
      estado: <%= estado %>,
      tipo: $("#tipo").val(),
      cidade: <%= cidade %>
    },
    dataType: 'script'
	})
<% end %>
$("div.endereco img").remove()
